{
  "name": "dogsled",
  "version": "0.1.4",
  "description": "Server-side JSX block render caching for React applications",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha -w test/helpers/browser.js test/*.spec.js",
    "prepublish": "mocha test/helpers/browser.js test/*.spec.js; babel ./src --out-dir ./lib --source-maps --presets es2015,react --plugins babel-plugin-add-module-exports"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/truecar/dogsled"
  },
  "keywords": [
