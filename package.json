    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-cli": "^6.18.0",
    "chai": "^3.5.0",
    "enzyme": "^2.5.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "sleep": "^4.0.0"
